{
  "Evolve.ConnectionString": "Server=${EVOLVE_HOST};Port=5432;Database=myDataBase;User Id=${EVOLVE_DB_USER};Password=${EVOLVE_DB_PWD};",
  "Evolve.Driver": "npgsql",
  "Evolve.Locations": "migration;dataset;",
  "Evolve.EraseDisabled": "true",
  "Evolve.Command": "ERase",
  "Evolve.EraseOnValidationError": "TRue",
  "Evolve.Encoding": "utf-16",
  "Evolve.SqlMigrationPrefix": "Ver",
  "Evolve.SqlMigrationSeparator": "@",
  "Evolve.SqlMigrationSuffix": ".query",
  "Evolve.Schemas": "my_shema",
  "Evolve.MetadataTableSchema": "my_metadata_schema",
  "Evolve.MetadataTableName": "metadata_store",
  "Evolve.PlaceholderPrefix": "@{",
  "Evolve.PlaceholderSuffix": "@}",
  "Evolve.TargetVersion": "2_1_0",
  "Evolve.StartVersion": "1_1_0",

  "Evolve.Placeholder.Schema": "my_schema",
  "Evolve.Placeholder.Pwd": "password"
}