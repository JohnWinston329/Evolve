<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="Evolve.ConnectionString" value="Server=${EVOLVE_HOST};Port=5432;Database=myDataBase;User Id=${EVOLVE_DB_USER};Password=${EVOLVE_DB_PWD};" />
    <add key="Evolve.Driver" value="npgsql" />
    <add key="Evolve.Encoding" value="" />
    <add key="Evolve.Schemas" value="my_shema" />
    <add key="Evolve.EraseDisabled" value="false" />
    <add key="Evolve.EraseOnValidationError" value="Fals" />
    <add key="Evolve.EnableClusterMode" value="true" />
    <add key="Evolve.OutOfOrder" value="true" />
  </appSettings>
</configuration>